#METROPOLIS ROUTER CONFIG

enable
conf t

int fa0/0
ip addr 10.64.0.62 255.255.255.128
no shut
exit

int fa0/1
ip addr 10.64.62.193 255.255.255.248
no shut
exit

int fa1/0
ip addr 10.64.62.201 255.255.255.248
no shut
exit


int fa0/1.10
encapsulation dot1q 10
ip addr 10.64.62.1 255.255.255.128
no shut
exit

int fa1/0.10
encapsulation dot1q 10
ip addr 10.64.62.129 255.255.255.192
no shut
exit

int loop0
ip addr 10.64.62.208 255.255.255.255
no shut
exit

ip route 0.0.0.0 0.0.0.0 10.64.0.64

router ospf 1
network 10.64.0.0 0.0.0.255 area 0
network 10.64.62.128 0.0.0.127 area 0
network 10.64.62.0 0.0.0.63 area 0
network 10.64.62.192 0.0.0.7 area 0
network 10.64.62.200 0.0.0.7 area 0
network 10.64.62.208 0.0.0.0 area 0
exit



enable secret cisco

hostname METROPOLIS
ip domain-name bornstallings

crypto key generate rsa
1024

line vty 0 4
transport input ssh
password cisco
login
exit

line con 0
password cisco
login
exit

service password-encryption
